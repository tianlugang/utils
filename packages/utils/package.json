{
  "name": "@tlg/utils",
  "version": "1.1.1-alpha.4+5963000",
  "description": "my utils",
  "license": "MIT",
  "files": [
    "lib/*",
    "dist/*",
    "types/*"
  ],
  "main": "lib/node/index.js",
  "browser": {
    "crypto": false
  },
  "scripts": {
    "api": "api-extractor run --local --verbose",
    "lib": "tsc --build",
    "build": "tsc --build && rollup -c",
    "build:test": "tsc --build tests",
    "clean": "rimraf ./lib && rimraf *.tsbuildinfo && rimraf ./types && rimraf ./dist",
    "clean:test": "rimraf tests/build",
    "docs": "typedoc --options tdoptions.json src",
    "minimize": "terser dist/*/index.js -c -m -o dist/*/index.min.js",
    "test": "npm run test:firefox",
    "test:chrome": "cd tests && karma start --browsers=Chrome",
    "test:firefox": "cd tests && karma start --browsers=Firefox",
    "test:ie": "cd tests && karma start --browsers=IE",
    "watch": "tsc --build --watch"
  },
  "devDependencies": {
    "@tlg/builder": "^1.1.1-alpha.4+5963000",
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "chai": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "mocha": "^6.2.2",
    "rimraf": "^2.5.2",
    "rollup": "^2.22.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "tslib": "1.10.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  },
  "gitHead": "596300067474b707fe9fa54439138528c1192e53"
}
